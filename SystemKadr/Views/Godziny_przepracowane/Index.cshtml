@model IEnumerable<SystemKadr.Models.Godziny_przepracowane>

@{
    ViewBag.Title = "Godziny przepracowane";
}

<h2>Godziny przepracowane</h2>

<p>
    @Html.ActionLink("Dodaj godziny", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pracownicy.Imie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pracownicy.Nazwisko)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pracownicy.Identyfikator)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Miesiac)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Godziny)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pracownicy.Imie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pracownicy.Nazwisko)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pracownicy.Identyfikator)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Miesiac)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Godziny)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id_wpisu }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id_wpisu }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id_wpisu })
        </td>
    </tr>
}

</table>
