@model IEnumerable<SystemKadr.Models.Wyplaty>
@{
    if (Session["Identyfikator"] == null)
    {
        Response.Redirect("~/Home/Index");
    }
}
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Miesiac)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Wyplata)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pracownicy.Imie)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Miesiac)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wyplata)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pracownicy.Imie)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id_tranzakcji }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id_tranzakcji }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id_tranzakcji })
            </td>
        </tr>
    }

</table>
